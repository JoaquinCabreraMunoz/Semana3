package westmarket1.pkg1;
import java.util.ArrayList;
        
        public class Categoria {
    private int codigo;
    private String nombre;

    public Categoria(int codigo, String nombre) {
        this.codigo = codigo;
        this.nombre = nombre;
    }

    public int getCodigo() {
        return codigo;
    }

    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }
private Categoria cat1 = new Categoria(1, "Bebidas");    
private Categoria cat2 = new Categoria(2, "Congelados");
private Categoria cat3 = new Categoria(3, "Lácteos");
private Categoria cat4 = new Categoria(4, "Aseo");

    public Categoria getCat1() {
        return cat1;
    }

    public void setCat1(Categoria cat1) {
        this.cat1 = cat1;
    }

    public Categoria getCat2() {
        return cat2;
    }

    public void setCat2(Categoria cat2) {
        this.cat2 = cat2;
    }

    public Categoria getCat3() {
        return cat3;
    }

    public void setCat3(Categoria cat3) {
        this.cat3 = cat3;
    }

    public Categoria getCat4() {
        return cat4;
    }

    public void setCat4(Categoria cat4) {
        this.cat4 = cat4;
    }
}


Categoria cat1 = new Categoria(1, "Bebidas");    
Categoria cat2 = new Categoria(2, "Congelados");
Categoria cat3 = new Categoria(3, "Lácteos");
Categoria cat4 = new Categoria(4, "Aseo");
}
